{
  "name": "@ui5/openui5-snapshot-test",
  "private": true,
  "version": "2.0.1",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:SAP/ui5-typescript.git",
    "directory": "test-packages/openui5-snapshot-test"
  },
  "dependencies": {
    "@ui5/dts-generator": "2.0.1",
    "fs-extra": "9.0.1",
    "lodash": "4.17.15",
    "node-fetch": "2.6.0",
    "typescript": "3.9.7"
  },
  "devDependencies": {},
  "scripts": {
    "ci": "npm-run-all test:*",
    "test:": "mocha \"./test/**/*spec.js\"",
    "test:generate": "npm-run-all sdk:* dts:*",
    "sdk:download": "node ./lib/download-sdk.js",
    "sdk:format": "prettier --write \"input-sdk/*.json\"",
    "dts:generate": "node ./lib/generate-dts.js",
    "dts:format": "prettier --write \"output-dts/*.d.ts\"",
    "dts:compile": "tsc -p ."
  },
  "mocha": {
    "diff": false
  },
  "snapshot": {
    "version": "1.78.1",
    "libs": [
      "sap.ui.core",
      "sap.m",
      "sap.f",
      "sap.tnt"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
