{
  "name": "@ui5/openui5-snapshot-test",
  "private": true,
  "version": "3.0.2",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:SAP/ui5-typescript.git",
    "directory": "test-packages/openui5-snapshot-test"
  },
  "dependencies": {
    "@ui5/dts-generator": "link:../../packages/dts-generator",
    "fs-extra": "11.2.0",
    "lodash": "4.17.21",
    "node-fetch": "3.3.2",
    "typescript": "5.4.5"
  },
  "devDependencies": {
    "@types/jquery": "3.5.13",
    "@types/qunit": "2.5.4"
  },
  "scripts": {
    "ci": "npm-run-all test",
    "test": "mocha \"./test/**/*spec.js\"",
    "re-generate": "npm-run-all sdk:* dts:*",
    "sdk:download": "node ./lib/download-sdk.js",
    "sdk:format": "prettier --write \"input-sdk/*.json\"",
    "dts:generate": "node ./lib/generate-dts.js",
    "dts:verify": "tsc -p ."
  },
  "mocha": {
    "diff": false
  },
  "snapshot": {
    "version": "1.120.12",
    "libs": [
      "sap.ui.core",
      "sap.m",
      "sap.f",
      "sap.tnt"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
